<Window x:Class="TablePet.Win.Start.EditWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="编辑启动项" Height="300" Width="400">
    <Grid>
        <ListBox Name="LaunchItemList" Margin="10" DisplayMemberPath="Name" />
        <Button Content="添加" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="10" Click="AddItem_Click"/>
        <Button Content="删除" HorizontalAlignment="Center" VerticalAlignment="Bottom" Margin="10" Click="RemoveItem_Click"/>
        <Button Content="保存" HorizontalAlignment="Right" VerticalAlignment="Bottom" Margin="10" Click="SaveItems_Click"/>
    </Grid>
</Window>